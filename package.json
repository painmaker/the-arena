{
	"name": "the_arena",
	"private": true,
	"scripts": {
		"launch": "node scripts/launch.js",
		"build": "run-p build:*",
		"build:panorama": "node --preserve-symlinks node_modules/webpack/bin/webpack.js --config content/panorama/webpack.config.js",
		"build:vscripts": "tstl --project game/scripts/vscripts/tsconfig.json",
		"dev": "run-p dev:*",
		"dev:panorama": "node --preserve-symlinks node_modules/webpack/bin/webpack.js --config content/panorama/webpack.config.js --watch",
		"dev:vscripts": "tstl --project game/scripts/vscripts/tsconfig.json --watch",
		"lint": "eslint content/panorama/src/hud/components/**/*",
		"lint:fix": "eslint content/panorama/src/hud/components/**/* --fix"
	},
	"devDependencies": {
		"@aabao/webpack-panorama": "0.2.1",
		"@moddota/find-steam-app": "1.1.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"css-loader": "6.7.1",
		"css-minimizer-webpack-plugin": "4.0.0",
		"eslint": "8.18.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.30.1",
		"eslint-plugin-react-hooks": "4.6.0",
		"file-loader": "6.2.0",
		"fork-ts-checker-webpack-plugin": "7.2.11",
		"fs-extra": "10.1.0",
		"mini-css-extract-plugin": "2.6.1",
		"npm-run-all": "4.1.5",
		"prettier": "2.7.1",
		"terser-webpack-plugin": "5.3.3",
		"ts-loader": "9.3.0",
		"typescript": "4.7.4",
		"typescript-to-lua": "1.6.0",
		"webpack": "5.73.0",
		"webpack-cli": "4.10.0"
	},
	"dependencies": {
		"@moddota/dota-lua-types": "4.19.0",
		"@moddota/panorama-types": "1.20.0",
		"@types/react": "18.0.14",
		"@types/react-dom": "18.0.5",
		"@types/react-reconciler": "0.28.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-reconciler": "0.29.0"
	}
}
