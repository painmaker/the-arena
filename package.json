{
	"name": "the_arena",
	"private": true,
	"scripts": {
		"launch": "node scripts/launch.js",
		"build": "run-p build:*",
		"build:panorama": "node --max-old-space-size=8192 --preserve-symlinks node_modules/webpack/bin/webpack.js --config content/panorama/webpack.config.js",
		"build:vscripts": "tstl --project game/scripts/vscripts/tsconfig.json",
		"dev": "run-p dev:*",
		"dev:panorama": "node --max-old-space-size=8192 --preserve-symlinks node_modules/webpack/bin/webpack.js --config content/panorama/webpack.config.js --watch",
		"dev:vscripts": "tstl --project game/scripts/vscripts/tsconfig.json --watch"
	},
	"devDependencies": {
		"@aabao/webpack-panorama": "0.2.1",
		"@moddota/find-steam-app": "1.1.0",
		"css-loader": "6.5.1",
		"css-minimizer-webpack-plugin": "3.2.0",
		"file-loader": "6.2.0",
		"fork-ts-checker-webpack-plugin": "6.3.3",
		"fs-extra": "10.0.0",
		"mini-css-extract-plugin": "2.4.5",
		"npm-run-all": "4.1.5",
		"terser-webpack-plugin": "5.2.4",
		"ts-loader": "9.2.5",
		"typescript": "4.4.3",
		"typescript-to-lua": "1.0.1",
		"webpack": "5.40.0",
		"webpack-cli": "4.8.0"
	},
	"dependencies": {
		"@moddota/dota-lua-types": "4.13.0",
		"@moddota/panorama-types": "1.11.0",
		"@types/react": "17.0.19",
		"@types/react-redux": "7.1.18",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-panorama": "0.1.2",
		"react-redux": "7.2.4",
		"redux": "4.1.1",
		"redux-saga": "1.1.3"
	}
}
